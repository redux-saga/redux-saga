{
  "name": "@redux-saga/core",
  "version": "1.4.2",
  "description": "Saga middleware for Redux to handle Side Effects",
  "main": "dist/redux-saga-core.cjs.js",
  "module": "dist/redux-saga-core.esm.js",
  "files": [
    "dist",
    "effects",
    "types"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/redux-saga-core.cjs.mjs",
        "default": "./dist/redux-saga-core.cjs.js"
      },
      "development": {
        "module": "./dist/redux-saga-core.development.esm.js",
        "import": "./dist/redux-saga-core.development.cjs.mjs",
        "default": "./dist/redux-saga-core.development.cjs.js"
      },
      "module": "./dist/redux-saga-core.esm.js",
      "import": "./dist/redux-saga-core.cjs.mjs",
      "default": "./dist/redux-saga-core.cjs.js"
    },
    "./effects": {
      "types": {
        "import": "./effects/dist/redux-saga-core-effects.cjs.mjs",
        "default": "./effects/dist/redux-saga-core-effects.cjs.js"
      },
      "development": {
        "module": "./effects/dist/redux-saga-core-effects.development.esm.js",
        "import": "./effects/dist/redux-saga-core-effects.development.cjs.mjs",
        "default": "./effects/dist/redux-saga-core-effects.development.cjs.js"
      },
      "module": "./effects/dist/redux-saga-core-effects.esm.js",
      "import": "./effects/dist/redux-saga-core-effects.cjs.mjs",
      "default": "./effects/dist/redux-saga-core-effects.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#is-development": {
      "development": "./src/resolved-conditions/development.js",
      "default": "./src/resolved-conditions/production.js"
    }
  },
  "scripts": {
    "test:types": "dtslint types"
  },
  "repository": "https://github.com/redux-saga/redux-saga/tree/main/packages/core",
  "keywords": [
    "javascript",
    "redux",
    "middleware",
    "saga",
    "effects",
    "side effects"
  ],
  "author": "Yassine ELOUAFI <yelouafi@gmail.com>",
  "contributors": [
    "Mateusz Burzy≈Ñski <mateuszburzynski@gmail.com> (https://github.com/Andarist)",
    "Mikhail Shustov <restrry@gmail.com> (https://github.com/restrry)",
    "Shi Feichao <842351815@qq.com> (https://github.com/shinima)",
    "Daniel Lytkin <dan.lytkin@gmail.com (https://github.com/aikoven)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redux-saga/redux-saga/issues"
  },
  "homepage": "https://redux-saga.js.org/",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/redux-saga"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@redux-saga/deferred": "^1.3.1",
    "@redux-saga/delay-p": "^1.3.1",
    "@redux-saga/is": "^1.2.1",
    "@redux-saga/symbols": "^1.2.1",
    "@redux-saga/types": "^1.3.1",
    "typescript-tuple": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "dtslint": "3.5.2",
    "jest": "^29.7.0",
    "mitt": "^1.1.2",
    "redux": "^4.0.4",
    "typescript": "^3.6.4"
  },
  "types": "./dist/redux-saga-core.cjs.d.ts",
  "typesVersions": {
    "<3.6": {
      "*": [
        "../types/pre-ts3.6/*"
      ]
    },
    "<4.2": {
      "*": [
        "../types/pre-ts4.2/*"
      ]
    }
  },
  "preconstruct": {
    "entrypoints": [
      "./index.js",
      "./effects.js"
    ]
  }
}
